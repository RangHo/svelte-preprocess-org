    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.5",
    "globals": "^16.4.0",
    "ox-svelte": "catalog:",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.41.4",
    "svelte-check": "^4.3.3",
    "svelte-preprocess-org": "workspace:*",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "npm:rolldown-vite@^7.1.19"
